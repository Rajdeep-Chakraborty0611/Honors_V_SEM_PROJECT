{
  "expo": {
    "name": "WeatherApp",
    "slug": "weatherapp",
    "platforms": ["ios", "android", "web"],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "enabled": true
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "com.example.weatherapp",
      "supportsTablet": true
    },
    "android": {
      "package": "com.example.weatherapp"
    }
  }
}
